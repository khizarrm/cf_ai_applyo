		"db:studio": "drizzle-kit studio",
		"db:push:local": "drizzle-kit push --dialect=sqlite --url=./.wrangler/state/v3/d1/miniflare-D1DatabaseObject/applyo.sqlite",
		"db:push:remote": "drizzle-kit push"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"drizzle-kit": "^0.31.5",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.42.1"
	},
	"dependencies": {
		"agents": "^0.2.9",
		"better-auth-cloudflare": "^0.2.7"
	}
